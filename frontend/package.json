{
  "name": "remix-express-docker",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server/index.ts --outdir=./server",
    "build:css": "npm run generate:css -m",
    "watch": "run-p watch:*",
    "watch:remix": "remix watch",
    "watch:server": "npm run build:server -- --watch",
    "watch:css": "npm run generate:css -- -w",
    "generate": "run-p generate:*",
    "generate:css": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "npm run test -- --update",
    "lint": "eslint app --ext ts,tsx",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@remix-run/express": "1.6.8",
    "@remix-run/node": "1.6.8",
    "@remix-run/react": "1.6.8",
    "compression": "1.7.4",
    "express": "4.18.1",
    "morgan": "1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.6.8",
    "@remix-run/eslint-config": "1.6.8",
    "@remix-run/serve": "1.6.8",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/morgan": "1.9.3",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-test-renderer": "18.0.0",
    "autoprefixer": "10.4.8",
    "concurrently": "7.3.0",
    "esbuild": "0.15.2",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-tailwindcss": "3.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "vitest": "0.21.1"
  },
  "engines": {
    "node": ">=18"
  }
}
